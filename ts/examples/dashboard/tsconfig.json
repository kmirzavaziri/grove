{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@grove/grove": ["../../packages/grove/src"],
      "@grove/grovex": ["../../packages/grovex/src"]
    }
  },
  "include": ["src/**/*", "vite.config.ts"]
}